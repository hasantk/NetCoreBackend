{
  //Token' yapısında olması zorunlu alanlar
  "TokenOptions": {
    "Audience": "hasan@hasan.com", //(Anlamı=Kitle)Herhangi istediğimiz web adresi verilebilir
    "Issuer": "hasan@hasan.com", //(Anlamı=Uygulayan)
    "AccessTokenExpiration": 10, //Token 'nın geçerlilik süresi (10dk)
    "SecurityKey": "mysupersecretkeymysupersecretkey" //Asp.net kullancağı anahtarın ismi.
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
